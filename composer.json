{
  "name": "dostoevski/preparer",
  "description": "Prepare xml for generate doc",
  "type": "library",
  "keywords": [
    "php",
    "documentor"
  ],
  "license": "MIT License",
  "homepage": "https://github.com/dostoevski/preparer",
  "authors": [
    {
      "name": "Malofeykin Andrey",
      "email": "and-rey2@yandex.ru"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "zendframework/zend-coding-standard": "~1.0.0",
    "phpunit/phpunit": "6.5.5",
    "php-coveralls/php-coveralls": "2.0",
    "phpunit/phpcov":"4.0.4",
    "squizlabs/php_codesniffer": "2.9.1"
  },
  "autoload": {
    "psr-4": {
      "Dostoevski\\Preparer\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Dostoevski\\Preparer\\PhpUnit\\Test\\": "test/phpunit/tests"
    },
    "classmap": [
      "test/"
    ]
  }
}